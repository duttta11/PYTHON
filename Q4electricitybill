a=int(input("enter electricity units="))
if(a>1 and a<=100):
  a*3
  print("electricity bill is=",a)
elif(a>100 and a<=200):
  a=a-100
  b=a*5+300
  print("electricity bill is=",b)
elif(a>200 and a<=300):
  a=a-200
  c=a*8+800
  print("electricity bill is=",c)
elif(a>300 and a<=400):
  a=a-300
  d=a*12+1600
  print("electricty bill is=",d)
elif(a<400):
  a=a-400
  e=a*16+1600+1200
  print("electricity bill is=")
